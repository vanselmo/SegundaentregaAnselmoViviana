<h1 class="h2">Todos los productos</h1>

<div class="productsContainer">
    {{#each products}}
    <div class="product row">
        <div>
            <p>{{this.title}}</p>
            <p>{{this.description}}</p>
            <p>{{this.price}}</p>
            <p>{{this.code}}</p>
            <p>{{this.stock}}</p>
            <p>{{this.category}}</p>
            <p>{{this.status}}</p>
            <p>{{this.thumbnails}}</p>
        </div>
    </div>
    {{/each}}
</div>